import random
def is_sorted(data):
    for i in range(len(data) - 1):
        print(i)
        print(data[i]>data[i+1])
        if data[i] > data[i + 1]:
            
            
            return False
    return True

def bogosort(data):
    #k=0
    while not is_sorted(data):
         #k+=1
         random.shuffle(data)
    #print(k)
    return data
    
print(bogosort(data))
